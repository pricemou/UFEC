<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="email_template_prof" model="mail.template">
            <field name="name"> Template proffesur</field>
            <field name="model_id" ref='ufec.model_ufec_porfesseurs' />
            <field name="email_to">${ctx['mail_to']|safe} </field>
            <field name="subject">${object.l_name} Invitation</field>
            <field name="subject"><![CDATA[
                    <p>
                        <strong>Dear ${object.f_nom} ${object.prenom}</strong>
                    </p>
                    <p>We invite you to be present in the week's assembly.</p>
                    <p>Best regards,</p>
            ]]>
            </field>
        </record>
    </data>
</odoo>